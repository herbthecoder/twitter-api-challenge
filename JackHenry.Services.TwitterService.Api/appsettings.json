{

  "Serilog": {
    "Using": [ "Serilog.Sinks.Console"],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level}]{MachineName}{Message}{NewLine}{Exception}"
        }
      }
    ],
    "Properties": {
      "Application": "Twitter Service"
    }
  },
  "AllowedHosts": "*",
  "CacheDuration": ""
}
